    "url": "http://github.com/thinkingmedia/readme"
  },
  "directories": {
    "bin": "./bin"
  },
  "dependencies": {
    "lodash": "~3.0.0",
    "dustjs-linkedin": "~2.6.0",
    "string": "~3.0.0",
    "q": "~1.2.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "expect.js": "^0.3.1",
    "mocha": "^2.2.1"
  },
  "config" : {
    "blanket": {
      "pattern": "src",
      "data-cover-never": "node_modules"
    }
  }
