{
  "name": "readme",
  "version": "1.0.0",
  "description": "A readme file generator",
  "author": "Mathew Foscarini <support@thinkingmedia.ca>",
  "main": "./src/readme.js",
  "keywords": [
    "readme",
    "generator",
    "command line"
  ],
  "maintainers": [
    {
      "name": "Mathew Foscarini",
      "email": "support@thinkingmedia.ca"
    }
  ],
  "license": [
    {
      "type": "MIT",
      "url": "https://github.com/gruntjs/grunt-cli/blob/master/LICENSE-MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/thinkingmedia/readme/issues"
  },
  "preferGlobal": "true",
